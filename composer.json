{
  "name": "blueways/bw-guild",
  "type": "typo3-cms-extension",
  "description": "Extends TYPO3 for the management of guilds",
  "homepage": "https://blueways.de",
  "license": [
    "proprietary"
  ],
  "keywords": [
    "TYPO3 CMS"
  ],
  "require-dev": {
    "typo3/cms-base-distribution": "^10.0",
    "blueways/bw-email": "^2.0",
    "bk2k/bootstrap-package": "^11.0",
    "typo3/cms-styleguide": "^10.0",
    "typo3/cms-scheduler": "^10.0"
  },
  "extra": {
    "typo3/cms": {
      "cms-package-dir": "{$vendor-dir}/typo3/cms",
      "web-dir": "public"
    }
  },
  "config": {
    "bin-dir": "bin",
    "vendor-dir": "vendor",
    "platform": {
      "php": "7.4"
    }
  },
  "autoload": {
    "psr-4": {
      "Blueways\\BwGuild\\": "Classes/"
    }
  },
  "require": {
    "ext-iconv": "*",
    "ext-pdo": "*",
    "ext-json": "*",
    "ext-gd": "*",
    "typo3/cms-core": "^9.0 || ^10.0"
  },
  "authors": [
    {
      "name": "Maik Schneider",
      "homepage": "https://blueways.de",
      "email": "m.schneider@blueways.de",
      "role": "Developer"
    }
  ]
}
